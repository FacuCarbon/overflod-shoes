<%- include('partials/head')  %>


<%- include('partials/header') %> 

    

  
  
    <div class="container">
      <div class="row calzado mt-5">
        <div class="col-12">
          
          <h1 class="text-warning titulo-listado" >Resultados para: <%= busqueda %></h1> </div>
        <% productos.forEach(producto => { %>
        <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 fotoArticulos">
          <!-- CALZADO 1-->
          <div class="p-1 text-center" style="height: 350px;">
            <figure>
              <a href="/productos/detalle/<%= producto.id %>"><img src="/img/productosAgregados/<%= producto.imagenes %>" alt=""></a>
            </figure>
            <article>
              <h3> <%= producto.nombre %></h3>
              <p class="marca"><%= producto.marca.nombre %> </p>
             
            </article>
            <h4 class="precio"> $<%= producto.precio %> </h4>
    
          </div>
        </div>
        <% }) %>
      </div>
    </div>
        <%- include('partials/footer')  %>